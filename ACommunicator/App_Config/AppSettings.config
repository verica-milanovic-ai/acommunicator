<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!--default ones-->
  <add key="webpages:Version" value="3.0.0.0" />
  <add key="webpages:Enabled" value="false" />
  <add key="ClientValidationEnabled" value="true" />
  <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  <!--/default ones-->

  <!--user defined-->
  <add key="EndUserProfilePictureNamePattern" value="a_{0}_end_{1}_{2}{3}"/>
  <add key="EndUserProfilePictureDirectory" value="~/profile_pictures"/>
  <add key="DefaultProfilePictureFileName" value="/Content/imgs/default.png"/>
  <!--/user defined-->
</appSettings>